<div class="col-md-10 col-md-offset-1">
	<div class="new-read-form">
	<h1>Edit Book</h1>
	<%= simple_form_for @read, html: {multipart: true} do |f|%>
			<%= f.file_field :read_img %>
			<%= f.select :category_id, @categories %>
	 	 	<%= f.input :title, label: "Read Title" %>
	  		<%= f.input :description %>
	  		<%= f.input :writer %>
		  	<%= f.button :submit %>
	<% end %>
	</div>
</div>